{
  "version": 2,
  "builds": [
    {
      "src": "api/items.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/items.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
